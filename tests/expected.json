bob_johnson = Authors.objects.create(first_name="Bob", last_name="Johnson", id=1)
bob_thorton = Authors.objects.create(first_name="Bob", last_name="Thorton", id=2)
george_costanza = Authors.objects.create(first_name="George", last_name="Costanza")

random_house = Publisher.objects.create(name="Random House", id=1)
penguin = Publisher.objects.create(name="Penguin", id=2)

b1 = Book.objects.create(publisher=random_house, title="Book 1", id=1)
b1.add(bob_johnson, bob_thorton)

b2 = Book.objects.create(publisher=random_house, title="Book 2", id=2)
b2.add(bob_johnson, george_costanza)

b3 = Book.objects.create(publisher=random_house, title="Book 3", id=3)
b3.add(george_costanza)

b4 = Book.objects.create(publisher=penguin, title="Book 4", id=4)
b4.add(bob_thorton, george_costanza)

b5 = Book.objects.create(publisher=penguin, title="Book 5", id=5)
b5.add(bob_johnson, bob_thorton, george_costanza)

b6 = Book.objects.create(publisher=penguin, title="Ghost Protocol", id=6)

